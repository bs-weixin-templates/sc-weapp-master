.sc-sticky-item-header {
  background: #eee;
  font-size: 14px;
  width: 100%;
  height: 32px;
  line-height: 32px;
}
.sc-sticky-item-content {
  font-size: 14px;
}
.sc-sticky-title {
  width: 100%;
  padding: 0 15px;
  box-sizing: border-box;
  background: #eee;
}
.sc-sticky-fixed .sc-sticky-title {
  position: fixed;
  top: 0;
}
