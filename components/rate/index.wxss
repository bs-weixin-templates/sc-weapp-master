.sc-rate {
  margin: 0;
  padding: 0;
  font-size: 20px;
  display: inline-block;
  vertical-align: middle;
  font-weight: normal;
  font-style: normal;
}
.sc-rate-hide-input {
  display: none;
}
.sc-rate-star {
  display: inline-block;
  color: #e9e9e9;
}
.sc-rate-current {
  color: #f5a623;
}
.sc-rate-text {
  display: inline-block;
  vertical-align: middle;
  margin-left: 6px;
  font-size: 14px;
}
