.sc-divider {
  width: 100%;
  text-align: center;
  font-size: 12px;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}
.sc-divider-line {
  position: absolute;
  left: 0;
  width: 100%;
  height: 1rpx;
  background-color: #f8f8f8;
  top: 50%;
}
.sc-divider-content {
  background: #fff;
  position: relative;
  z-index: 1;
  display: inline-block;
  padding: 0 10px;
}
